@model IEnumerable<Dishe>
<h1 class="display-4">Welcome to CRUDelicious!</h1>
<a asp-controller="Home" asp-action="New">Add a new Dish</a>
<h3 class="display-6">Check out some recent dishes:</h3>
<hr>
@foreach(var dishe in Model)
{
    <p ><a asp-controller="Home" 
        asp-action="Show" 
        asp-route-dishesId="@dishe.DishesId">@dishe.Name</a> by @dishe.Chef</p>
}