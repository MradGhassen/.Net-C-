
<h3>Check out our roster of Chefs!</h3>
<hr>
<table class="table table-striped table-bordered">
    <thead>
        <th>Name</th>
        <th>Age</th>
        <th># of Dishes</th>
    </thead>
    <tbody>
        @foreach (var o in ViewBag.AllChefs)
        {
            <tr>
                    <td>@o.FirstName @o.LastName</td>
                    <td>@o.DOB</td>
                    <td>@o.DishesOwned.Count</td>
        </tr>
        }
    </tbody>
</table>
